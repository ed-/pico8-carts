-- cursor

function new_cursor()
  local ch = {x=1, y=1}
  
  ch.move = function(self, x, y)
    self.x = x self.y = y
    if (x < 1)  self.x = 1
    if (x > df) self.x = df
    if (y < 1)  self.y = 1
    if (y >= df) self.y = df
  end
  ch.up = function(self)
    self:move(self.x, self.y - 1)
  end
  ch.down = function(self)
    self:move(self.x, self.y + 1)
  end
  ch.left = function(self)
    self:move(self.x - 1, self.y)
  end
  ch.right = function(self)
    self:move(self.x + 1, self.y)
  end
  
  ch.draw = function(self)
    spr(6, (self.x * 8) + zo(), (self.y * 8) + zo())
  end
  
  return ch
end
