function change_difficulty()
  df += 4
  if df > 14 then
    df = 6
  end
  dset(0, df)
  make_diff_menu()
  game = new_game()
end

function make_diff_menu()
  local diff_str = ""
  if df == 6 then
    diff_str = "difficulty: six"
  elseif df == 10 then
    diff_str = "difficulty: ten"
  else
    diff_str = "difficulty: max"
  end
  menuitem(1, diff_str, function() change_difficulty(1) end)
end

function zo()
  local z = {[6]=32, [10]=16, [14]=0}
  return z[df]
end
