-- game

function new_game()

  local game = {}
  
  game.show_mode = "goal"
  
  game.goal = conway(df)
  game.goal:randomize(0.5)
  game.goal:step()
  
  game.guess = game.goal:copy()
  game.next = {}
  game.next = game.guess:copy()
  game.count = df * df
  
  game.ch = new_cursor()
    
  game.update = function(self)
    self.count = df * df
    self.next = game.guess:copy()
    self.next:step()
    self.count = self.goal:diff(self.next)
    if (btnp(0)) self.ch:left()
    if (btnp(1)) self.ch:right()
    if (btnp(2)) self.ch:up()
    if (btnp(3)) self.ch:down()
    
    if btnp(4) then
      self.guess:toggle(self.ch.x, self.ch.y)
    end
    
    if btnp(5) then
      self:toggle_mode()
    end
    
  end
  
  game.won = function(self)
    return self.count == 0
  end
  
  game.toggle_mode = function(self)
    if self.show_mode == "goal" then
      self.show_mode = "guess"
    else
      self.show_mode = "goal"
    end
    sfx(2)
  end
    
  game.draw_border = function(self)
    if self:won() then
      pal(1, rainbow)
    else
      pal()
    end
    mset( 0,      0,      3)
    mset(df + 1,  0,      5)
    mset( 0,     df + 1, 35)
    mset(df + 1, df + 1, 37)

			 for z=1,3 do
			   mset( z,      0,  0)
			 end
    for z=4,df do
      mset( z,      0,  4)
    end

    for z=1,df do
      mset( 0,      z, 19)
      mset(df + 1,  z, 21) 
    end
    
    for z=1,df-2 do
      mset( z, df + 1, 36)
    end
    for z=df-2,df do
			   mset( z, df + 1,  0)
			 end
    
    local z = zo()
    local z1 = z + (df * 8) + 8

    map(     0,      0,  z,  z, df + 2,      1)
    map(     0, df + 1,  z, z1, df + 2,      1)
    map(     0,      0,  z,  z,      1, df + 2)
    map(df + 1,      0, z1,  z,      1, df + 2)
  end

  game.draw = function(self)
    self:draw_border()
    local d = self.goal:diff(self.next)
    local xo = zo() + (8 * df) + 4
    local yo = zo() + (8 * df) + 10
    if (d > 9) xo -= 4
    if (d > 99) xo -= 4

    if self.show_mode == "guess" then
      self.next:draw_green()
      self.guess:draw_blue()
      self.ch:draw()
      print(self.show_mode, zo() + 10, zo() + 1, 7)
    elseif self.show_mode == "goal" then
      self.goal:draw_gray()
      self.guess:draw_blue()
      self.ch:draw()
      print(self.show_mode, zo() + 10, zo() + 1, 7)
    end
    print(self.count, xo, yo, 7)
  end
    
  return game
end
