-- slumberheart

function _init()
  cls()
  frame = 0
  rainbow = 8
  df = 0
  if cartdata("slumberheart_petri") then
    df = dget(0)
  end
  if (df == 0) then
    df = 6
    dset(0, df)
  end
  make_diff_menu()
  menuitem(2, "new game", function() game = new_game(10) end)
  menuitem(3, "instructions", function() show_instructions = 1 end)
  game = new_game()
  show_instructions = 0
end

function _update()
  frame = (frame + 1) % 30
  if frame % 5 == 0 then
    rainbow += 1
    if (rainbow > 13) rainbow = 8
  end
  if (show_instructions > 0) then
    instr_update()
  else
    game:update()
  end
end

function _draw()
  cls()
  if (show_instructions > 0) then
    instr_draw()
  else
    game:draw()
  end
end
