-- conway

function conway()
  local cc = {}
  cc.cells = {}
  for row=1,df do
    cc.cells[row] = {}
    for col=1,df do
      cc.cells[row][col] = 0
    end
  end
  
  cc.randomize = function(self, density)
    for row=1,df do
      for col=1,df do
        if rnd() < density then
          self.cells[row][col] = 1
        else
          self.cells[row][col] = 0
        end
      end
    end
  end
  
  cc.cell = function(self, row, col)
    if ((row < 1) or (row > df)) return 0
    if ((col < 1) or (col > df)) return 0
    return self.cells[row][col]
  end
  
  cc.toggle = function(self, row, col)
    if ((row < 1) or (row > df)) return
    if ((col < 1) or (col > df)) return
    
    local c = self.cells[row][col]
    sfx(c)
    self.cells[row][col] = 1 - c
  end
  
  cc.neighbors = function(self, r, c)
    local n = 0
    n += self:cell(r - 1, c - 1)
    n += self:cell(r - 1, c)
    n += self:cell(r - 1, c + 1)

    n += self:cell(r, c - 1)
    n += self:cell(r, c + 1)

    n += self:cell(r + 1, c - 1)
    n += self:cell(r + 1, c)
    n += self:cell(r + 1, c + 1)
    
    return n
  end

  cc.step = function(self)
    local nn = {}
    for row=1,df do
      nn[row] = {}
      for col=1,df do
        nn[row][col] = self:neighbors(row, col)
      end
    end
    local n = 0 c = 0
    for row=1,df do
      for col=1,df do
        c = self.cells[row][col]
        n = nn[row][col]
        if ((c == 0)  and (n == 3)) c = 1
        if ((c == 1) and ((n < 2) or (n > 3))) c = 0
        self.cells[row][col] = c
      end
    end
  end
  
  cc.draw_gray = function(self)
    pal(10, 7)
    pal(11, 6)
    pal( 3, 5)
    self:draw_green()
    pal() 
  end
  
  cc.draw_green = function(self)
    for row=1,df do
      for col=1,df do
        if self.cells[row][col] == 0 then
          mset(row, col, 0)
        else
          local mask = 0
          if (self:cell(row, col - 1) == 1) mask = bor(mask, 1)
          if (self:cell(row + 1, col) == 1) mask = bor(mask, 2)
          if (self:cell(row, col + 1) == 1) mask = bor(mask, 4)
          if (self:cell(row - 1, col) == 1) mask = bor(mask, 8)
          mset(row, col, mask + 48)
        end
      end
    end
    map(1, 1, zo() + 8, zo() + 8, df, df)
  end

  cc.draw_blue = function(self)
    for row=1,df do
      for col=1,df do
        if self.cells[row][col] == 0 then
          mset(row, col, 2)
        else
          mset(row, col, 1)
        end
      end
    end
    map(1, 1, zo() + 8, zo() + 8, df, df)
  end
  
  cc.copy = function(self)
    local newcc = conway()
    for row=1,df do
      for col=1,df do
        newcc.cells[row][col] = self.cells[row][col]
      end
    end
    return newcc
  end
  
  cc.diff = function(self, other)
    local count = 0
    for row=1,df do
      for col=1,df do
        if (self:cell(row, col) != other:cell(row, col)) count += 1
      end
    end
    return count
  end
  
  return cc
end
